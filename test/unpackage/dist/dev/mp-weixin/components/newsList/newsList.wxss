.ul .li {
  width: 100%;
  height: 110rpx;
  background-color: #ffffff;
  margin: 3px auto;
  display: flex;
  align-items: center;
  justify-content: center;
}
.ul .li image {
  width: 68rpx;
  height: 68rpx;
  margin-right: 15px;
}
.ul .li .textBox {
  width: 400rpx;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
}
.ul .li .textBox .title {
  font-size: 30rpx;
  font-weight: 800;
  margin-bottom: 5px;
}
.ul .li .textBox .info {
  font-size: 26rpx;
  color: #979797;
}
.ul .li .time {
  font-size: 28rpx;
  color: #a9a9a9;
}

