.infoLists {
  display: flex;
  flex-wrap: wrap;
}
.infoLists .infoList {
  width: 320rpx;
  height: 500rpx;
  margin: 15rpx;
  padding: 10rpx;
  border: 1px solid #d7d7d7;
  border-radius: 10px;
}
.infoLists .infoList .middleBox {
  width: 100%;
  height: 100%;
  background-color: #ffffff;
  border-radius: 10px;
}
.infoLists .infoList .middleBox .imgBox {
  width: 100%;
  height: 77%;
  border-top-left-radius: 10px;
  border-top-right-radius: 10px;
  overflow: hidden;
}
.infoLists .infoList .middleBox .imgBox image {
  width: 100%;
  height: 100%;
}
.infoLists .infoList .middleBox .titleBox {
  width: 100%;
  height: 23%;
  font-size: 28rpx;
  font-weight: 600;
}
.infoLists .infoList .middleBox .titleBox .firstLine {
  width: 100%;
  height: 40%;
}
.infoLists .infoList .middleBox .titleBox .secondLine {
  width: 100%;
  height: 60%;
  display: flex;
}
.infoLists .infoList .middleBox .titleBox .secondLine .sleftBox {
  width: 65%;
  height: 100%;
  display: flex;
  align-items: center;
}
.infoLists .infoList .middleBox .titleBox .secondLine .sleftBox image {
  margin-left: 5px;
  width: 65rpx;
  height: 65rpx;
  border-radius: 50%;
  background-color: #f1e7e8;
}
.infoLists .infoList .middleBox .titleBox .secondLine .sleftBox text {
  margin-left: 3px;
}
.infoLists .infoList .middleBox .titleBox .secondLine .sRightBox {
  width: 35%;
  height: 100%;
  display: flex;
  align-items: center;
  font-weight: 500;
}
.infoLists .infoList .middleBox .titleBox .secondLine .sRightBox .iconzan {
  color: #deb2cd;
  font-size: 60rpx;
}
.infoLists .infoList .middleBox .titleBox .secondLine .sRightBox text {
  color: #a1a1a1;
  font-size: 28rpx;
}

