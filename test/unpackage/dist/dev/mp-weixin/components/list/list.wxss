.goodsList {
  width: 100%;
  height: auto;
}
.goodsList .infos {
  width: 90%;
  height: 240rpx;
  margin: 0 auto;
  display: flex;
  align-items: center;
  font-size: 24rpx;
  border-bottom: 2px solid #efefef;
  border-top: 1px solid #efefef;
}
.goodsList .infos image {
  width: 30%;
  height: 70%;
  border-radius: 5px;
}
.goodsList .infos .right-info {
  width: 68%;
  height: 70%;
  display: flex;
  margin-left: 2%;
  flex-direction: column;
}
.goodsList .infos .right-info .info-title {
  width: 100%;
  height: 20%;
  font-size: 30rpx;
  font-weight: 900;
}
.goodsList .infos .right-info .info-text {
  width: 100%;
  height: 69%;
  margin-top: 1%;
}
.goodsList .infos .right-info .iconBoxs {
  width: 100%;
  height: 9%;
  margin-top: 1%;
  display: flex;
  justify-content: flex-end;
}
.goodsList .infos .right-info .iconBoxs .revBox {
  margin-right: 20rpx;
}

