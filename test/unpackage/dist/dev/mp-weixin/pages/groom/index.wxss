page {
  background-color: #f6f6f6;
}
.F-Box {
  width: 80%;
  height: auto;
  margin: 0 auto;
}
.F-Box .pic {
  width: 100%;
  height: 400rpx;
  margin: 0 auto;
  border-bottom: 2px solid #d4d4d4;
  margin-top: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.F-Box .pic image {
  width: 65%;
  height: 250rpx;
}
.F-Box .bottomBox {
  width: 80%;
  height: auto;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin: 0 auto;
}
.F-Box .bottomBox .t1,
.F-Box .bottomBox .t2 {
  width: 200px;
  font-size: 28rpx;
  margin: 20px 0;
}
.F-Box .bottomBox .t1 {
  font-weight: 600;
  color: #8f8e8b;
  margin-bottom: 0;
}
.F-Box .bottomBox .t2 {
  margin-top: 0;
  font-size: 20rpx;
  color: #979797;
}
.F-Box .bottomBox button {
  width: 300rpx;
  height: 80rpx;
  background-image: linear-gradient(to right, #f4cfdb, #b3a9d8);
  font-size: 32rpx;
  font-weight: 700;
}

