.topBoxs {
  width: 100%;
  height: 600rpx;
  background-image: linear-gradient(to right top, #e999b6, #ae95cc);
}
.topBoxs .topBox {
  width: 100%;
  height: 400rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.topBoxs .topBox .headSculpture {
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
  background-color: #e8dbd3;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}
.topBoxs .topBox .headSculpture image {
  width: 98%;
  height: 98%;
  border-radius: 50%;
}
.topBoxs .topBox .nameSpace {
  width: 130rpx;
  height: 40rpx;
  margin-top: 10px;
  display: flex;
  justify-content: center;
  align-items: center;
}
.topBoxs .topBox .nameSpace text {
  width: 100rpx;
  text-align: center;
  font-size: 28rpx;
  color: #735c5e;
  font-weight: 600;
}
.topBoxs .bottomBox {
  width: 100%;
  height: 200rpx;
  display: flex;
}
.topBoxs .bottomBox .collectBox,
.topBoxs .bottomBox .goodBox,
.topBoxs .bottomBox .visitBox {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  font-size: 32rpx;
  font-weight: 600;
}
.topBoxs .bottomBox .collectBox .name,
.topBoxs .bottomBox .goodBox .name,
.topBoxs .bottomBox .visitBox .name {
  font-weight: 500;
  color: #8c7a80;
  font-size: 28rpx;
}
.news_list {
  width: 100%;
  height: auto;
  color: #83817c;
  font-size: 28rpx;
  font-weight: 600;
}
.news_list .news_top {
  width: 100%;
  height: 200rpx;
  background-color: #ffffff;
}
.news_list .news_top .mineInfo,
.news_list .news_top .secret {
  width: 80%;
  height: 100rpx;
  display: flex;
  align-items: center;
  margin: 0 auto;
  border-bottom: 1px solid #efefef;
}
.news_list .news_bottom {
  width: 100%;
  height: auto;
}
.news_list .news_bottom .userTickle,
.news_list .news_bottom .groom {
  width: 100%;
  height: 100rpx;
  background-color: #fff;
  margin-top: 16rpx;
}
.news_list .news_bottom .userTickle navigator,
.news_list .news_bottom .groom navigator {
  width: 80%;
  margin: 0 auto;
  height: 100%;
  display: flex;
  align-items: center;
}
.modify {
  position: flex;
  bottom: 0;
  width: 100%;
  height: 100px;
  background-color: pink;
}

