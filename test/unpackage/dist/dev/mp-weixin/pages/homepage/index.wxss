.hint {
  font-size: 10px;
}
.top-box {
  width: 100%;
}
.top-box .status-bar {
  z-index: 100;
  position: fixed;
  top: 0;
  width: 100%;
  background-image: linear-gradient(to right, #9b9ad6, #d18eb7);
}
.top-box .nav-bar {
  z-index: 100;
  position: fixed;
  width: 100%;
  height: 45px;
  background-image: linear-gradient(to right, #9b9ad6, #d18eb7);
  display: flex;
  align-items: center;
}
.top-box .nav-bar .navMidbar {
  margin-left: 20rpx;
  font-size: 14px;
  display: flex;
}
.top-box .nav-bar .navMidbar .placeBox {
  display: flex;
  align-items: center;
}
.top-box .nav-bar .navMidbar .placeBox .place {
  width: 45px;
  height: 22px;
}
.top-box .nav-bar .navMidbar .placeBox .pl {
  width: 14px;
  height: 26px;
  font-size: 8px;
  line-height: 26px;
}
.top-box .nav-bar .navMidbar .infoBox {
  width: 340rpx;
  height: 28px;
  background-color: #f8e4d9;
  display: flex;
  align-items: center;
  border-radius: 20px;
  margin-left: 20rpx;
  border: 1px solid #333;
  border-bottom: 0;
  border-left: 0;
}
.top-box .nav-bar .navMidbar .infoBox .sh {
  width: 20rpx;
  height: 20rpx;
  font-size: 10px;
  line-height: 20rpx;
  margin-left: 20rpx;
  color: #c2b8b1;
}
.top-box .nav-bar .navMidbar .infoBox .searchText {
  width: 320rpx;
  height: 100%;
  border-radius: 20px;
  margin-left: 10rpx;
  font-size: 10px;
  color: #948d88;
  text-indent: 16rpx;
}
.bottomBox {
  width: 95%;
  margin: 0 auto;
  margin-top: 5px;
  height: auto;
}
.bottomBox swiper {
  width: 100%;
  height: 340rpx;
}
.bottomBox swiper navigator {
  width: 100%;
  height: 100%;
}
.bottomBox swiper navigator image {
  width: 100%;
}
.bottomBox .navBox {
  margin-top: 5px;
  width: 100%;
  height: 200rpx;
  display: flex;
  align-items: center;
  background-color: #ffffff;
  border-radius: 10px;
  border-bottom: 1px solid #333;
  border-right: 1px solid #333;
}
.bottomBox .navBox .nav-icon {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.bottomBox .navBox .nav-icon image {
  width: 100rpx;
  height: 100rpx;
}
.bottomBox .navBox .nav-icon .nav-text {
  font-weight: 900;
  font-size: 14px;
  text-align: center;
}
.bottomBox .map-btn {
  width: 100%;
  height: auto;
  margin-top: 5px;
}
.bottomBox .map-btn navigator {
  width: 100%;
  height: 100%;
}
.bottomBox .map-btn navigator image {
  width: 100%;
  height: 99%;
  border-radius: 5px;
}
.bottomBox .info-box {
  margin-top: 5px;
  width: 100%;
  height: auto;
  background-color: #ffffff;
  border-radius: 10px;
}
.bottomBox .info-box .info-tabs {
  display: flex;
  width: 100%;
  height: 50rpx;
  padding-top: 10px;
}
.bottomBox .info-box .info-tabs .info-tab {
  flex: 1;
  text-align: center;
  font-size: 28rpx;
  font-weight: 900;
  color: #b5b5b5;
  position: relative;
}
.bottomBox .info-box .info-tabs .info-tab .lines {
  width: 100%;
  height: 4px;
  position: absolute;
  bottom: 0;
  border-bottom: 3px solid #c7c7c7;
}
.bottomBox .info-box .info-tabs .info-tab .line {
  width: 100%;
  height: 4px;
  position: absolute;
  bottom: 0;
  border-radius: 10px;
  background-image: linear-gradient(to right, #a798d0, #d18eb7);
}
.bottomBox .info-box .contents {
  width: 100%;
  height: auto;
}
.bottomBox .info-box .contents .infos {
  width: 90%;
  height: 240rpx;
  margin: 0 auto;
  display: flex;
  align-items: center;
  font-size: 24rpx;
  border-bottom: 2px solid #efefef;
  border-top: 1px solid #efefef;
}
.bottomBox .info-box .contents .infos image {
  width: 30%;
  height: 70%;
  border-radius: 5px;
}
.bottomBox .info-box .contents .infos .right-info {
  width: 68%;
  height: 70%;
  display: flex;
  margin-left: 2%;
  flex-direction: column;
}
.bottomBox .info-box .contents .infos .right-info .info-title {
  width: 100%;
  height: 20%;
  font-size: 30rpx;
  font-weight: 900;
}
.bottomBox .info-box .contents .infos .right-info .info-text {
  width: 100%;
  height: 69%;
  margin-top: 1%;
}
.bottomBox .info-box .contents .infos .right-info .iconBoxs,
.bottomBox .info-box .contents .infos .right-info .iconBox {
  width: 100%;
  height: 9%;
  margin-top: 1%;
  display: flex;
  justify-content: flex-end;
}
.bottomBox .info-box .contents .infos .right-info .iconBoxs .revBox,
.bottomBox .info-box .contents .infos .right-info .iconBox .revBox {
  margin-right: 20rpx;
}
.bottomBox .info-box .contents .infos .right-info .iconBox {
  height: 20px;
}
.bottomBox .info-box .contents .infos .right-info .iconBox .revBox {
  margin-right: 200rpx;
  font-size: 24rpx;
  font-weight: 800;
}
.bottomBox .info-box .contents .infos .right-info .iconBox .goodBox {
  display: flex;
  color: #a1a1a1;
}
.bottomBox .info-box .contents .infos .right-info .iconBox .goodBox .roundPic {
  width: 32rpx;
  height: 32rpx;
  margin-right: 2px;
}
.bottomBox .info-box .contents .myLifeBox {
  display: flex;
  flex-direction: column;
  height: auto;
  width: 100%;
}
.bottomBox .info-box .contents .myLifeBox .mineBox {
  width: 100%;
  height: 100px;
  background-color: #ffffff;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.bottomBox .info-box .contents .myLifeBox .mineBox .topInput {
  width: 90%;
  height: 50%;
  display: flex;
  align-items: center;
}
.bottomBox .info-box .contents .myLifeBox .mineBox .topInput .textBox {
  width: 480rpx;
  height: 50rpx;
  background-color: #fff7e4;
  border-radius: 20px;
}
.bottomBox .info-box .contents .myLifeBox .mineBox .topInput .textBox input {
  width: 90%;
  height: 100%;
  border: 0;
  color: #333;
  font-size: 12px;
  text-indent: 20rpx;
  margin-left: 10px;
}
.bottomBox .info-box .contents .myLifeBox .mineBox .topInput image {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  margin-right: 70rpx;
}
.bottomBox .info-box .contents .myLifeBox .mineBox .bottomIcon {
  display: flex;
  width: 90%;
  height: 50%;
  justify-content: flex-end;
  align-items: center;
  border-bottom: 2px solid #b2b2b2;
}
.bottomBox .info-box .contents .myLifeBox .mineBox .bottomIcon .b-icon {
  display: flex;
  align-items: center;
  flex-direction: column;
  margin-right: 50rpx;
  font-size: 10px;
  font-weight: 900;
}
.bottomBox .info-box .contents .myLifeBox .mineBox .bottomIcon .b-icon image {
  width: 75rpx;
  height: 55rpx;
}
.bottomBox .info-box .contents .myLifeBox .mineBox .bottomIcon .b-icon text {
  margin-top: 2px;
}
.bottomBox .info-box .contents .myLifeBox .mineBox .bottomIcon .b-icon:nth-child(3) image {
  width: 50rpx;
  height: 60rpx;
}
.bottomBox .info-box .contents .myLifeBox .mineBox .bottomIcon .b-icon:nth-child(4) image {
  width: 60rpx;
  height: 60rpx;
}
.bottomBox .info-box .contents .myLifeBox .mineInfos {
  width: 90%;
  margin: 0 auto;
}
.bottomBox .info-box .contents .myLifeBox .mineInfos .isNull {
  text-align: center;
  margin-top: 20rpx;
  font-size: 28rpx;
  font-weight: 600;
  color: #b1b1b1;
}
.bottomBox .info-box .contents .myLifeBox .mineInfos .mineInfo {
  width: 100%;
  height: auto;
}
.bottomBox .info-box .contents .myLifeBox .mineInfos .mineInfo tests {
  width: 100px;
  height: 100px;
  font-weight: 900;
  font-size: 20px;
}

